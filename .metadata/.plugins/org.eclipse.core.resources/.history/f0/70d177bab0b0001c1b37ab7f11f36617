package Quiz02_;

import java.util.Scanner;

public class Test01 {
	public int[] input1() {
		Scanner sc = new Scanner(System.in);
		System.out.print("첫번째 수 입력 : ");
		int num1 = sc.nextInt();
		System.out.print("두번째 수 입력 : ");
		int num2 = sc.nextInt();
		
		int[] arr = new int[2];
		
		arr[0] = num1; arr[1] = num2;
		
		result1(arr);
		return arr;
	}
	public int result1(int[] arr) {
			if(arr[0] > arr[1]) {
			System.out.println(arr[0]);
		}else {
			System.out.println(arr[1]);
		}
		return 0;
	}
}
