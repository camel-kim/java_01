package day06;

public class Ex01_Do_while__continue_break {
	public static void main(String[] args) {
		/*
			do ~ while 
			do{
			종속문장			//최초 1번은 어떠한 경우라도 실행
			while (조건식);
			}
		*/
		int su = 0, sum = 0;
		do {
			su++;
			sum += su;
		} while (su <= 9);
		System.out.println("sum : " + sum);
		/*
			continue, break
			반복문 안에서 사용한다.
			continue
			반복문의 위쪽으로 이동시켜준다
			break
			반복문을 빠져나간다.
		*/
		int i = 0;
		while (i < 5) {
			i++;
			if (i == 3) {
				System.out.println("실행");
				continue;
			}
		}System.out.println("i : " + i);
	}

}
