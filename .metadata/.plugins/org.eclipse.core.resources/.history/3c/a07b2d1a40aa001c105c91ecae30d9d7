package day06;

public class Ex01_Do_while {
	public static void main(String[] args) {
		/*
			do ~ while 
			do{
			종속문장			//최초 1번은 어떠한 경우라도 실행
			while (조건식);
			}
		*/
		int su = 0, sum = 0;
		do {
			su++;
			sum += su;
		} while (su <= 9);
		System.out.println("sum : " + sum);
		/*
			continue, break
			반복문 안에서 사용한다.
		*/
	}

}
