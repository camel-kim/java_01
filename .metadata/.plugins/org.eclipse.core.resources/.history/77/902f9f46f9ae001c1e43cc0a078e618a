package day08;

public class Ex01 {
	public static void main(String[] args) {
		//2차원 배열
		int arr[][] = new int[][] { { 1, 2 }, { 10, 20 }, { 30, 40 } };
		System.out.println(arr[0][0]);
		System.out.println(arr[1][1]);
		for(int i = 0; i < arr.length ; i++ );{
			for(int j = 0; j < arr[0].length; j++) {
				System.out.println(arr[i][j]+", ");
			}
		}
	}
}
