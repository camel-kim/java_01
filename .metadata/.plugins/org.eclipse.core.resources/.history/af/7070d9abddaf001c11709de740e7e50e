package day09;

public class Ex03 {
	public static void main(String[] args) {
		String str;
		str = "test";
		System.out.println(str);
		
		String str2 = new String();
		str2 = "test222";
		System.out.println(str2);
		//str. String 의 기능들 확인 필요..
		str2 = "java is Easy. 그리고 programming 할만하다";
		System.out.println("기본 : " + str2);
	}

}
