package Quiz02_;

import java.util.Scanner;

public class Ouiz02 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("첫번째 수를 입력하세요.");
		int num1 = sc.nextInt();
		System.out.println("두번째 수를 입력하세요.");
		int num2 = sc.nextInt();
		
		if(num1 > num2) {
			System.out.println(num1);
		}else {
			System.out.println(num2);
		}
		
	}

}
