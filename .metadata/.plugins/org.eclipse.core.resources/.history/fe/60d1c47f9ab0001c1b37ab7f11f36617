package Ex03_;

import java.util.Scanner;

public class Test03 {
	public int sumFunc(int num1, int n2) {
		int sum = 0;
		
		sum = num1 + n2;
		// 메서드를 만들 땐 강한결합 x 
		// 한가지의 기능만 사용하는 약한결합으로 만든다..?
		return sum;
		// 반환 자료형을 일치 시켜야함
		// public int 
		
	}
	public void input() {
		Scanner sc = new Scanner(System.in);
		int sum = 0, num1 = 0, num2 = 0;
		
		System.out.println("수 입력 : ");
		num1 = sc.nextInt();
		System.out.println("수 입력 : ");
		num2 = sc.nextInt();
		
		
	}

}
