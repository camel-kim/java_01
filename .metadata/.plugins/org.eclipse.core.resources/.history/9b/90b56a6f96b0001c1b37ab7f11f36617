public class Test03 {
	public void sumFunc() {
		int sum = 0, num1 = 0, num2 = 0;
		
		sum = num1 + num2;
		System.out.println("두 수 합 : " + sum);
		
		//메서드를 만들 땐 강한결합 x 
		//한가지의 기능만 사용하는 약한결합으로 만든다..?
	}
	public void input() {
		Scanner sc = new Scanner(System.in);
		int sum = 0, num1 = 0, num2 = 0;
		
		System.out.println("수 입력 : ");
		num1 = sc.nextInt();
		System.out.println("수 입력 : ");
		num2 = sc.nextInt();
		
		
	}

}
