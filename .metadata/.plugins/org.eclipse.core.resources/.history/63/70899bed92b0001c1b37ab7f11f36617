package Ex03_;

import java.util.Scanner;

public class Ex03 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num1, num2, sum = 0;
		
		System.out.println("수 입력 : ");
		num1 = sc.nextInt();
		System.out.println("수 입력 : ");
		num2 = sc.nextInt();
		
		sum = num1 + num2;
		System.out.println("두 수 합 : " + sum);
	}
	
}
